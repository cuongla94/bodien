{
  "name": "cuong-blog",
  "version": "0.1.0",
  "private": true,
  "description": "Cuong La's personal blog built with Next.js and Sanity CMS",
  "author": {
    "name": "Cuong La"
  },
  "license": "MIT",
  "keywords": [
    "blog",
    "personal-blog",
    "nextjs",
    "sanity",
    "react",
    "typescript",
    "bootstrap",
    "portfolio",
    "cms"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out node_modules/.cache",
    "preinstall": "npx only-allow npm",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@fontsource/fira-code": "^5.2.6",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/montserrat": "^5.2.6",
    "@fontsource/roboto": "^5.2.6",
    "@fontsource/source-sans-pro": "^5.2.5",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@lexical/link": "^0.32.1",
    "@lexical/markdown": "^0.32.1",
    "@lexical/react": "^0.32.1",
    "@lexical/rich-text": "^0.32.1",
    "@portabletext/react": "^3.2.1",
    "@radix-ui/react-select": "^2.2.5",
    "@sanity/client": "^6.15.7",
    "@sanity/image-url": "^1.0.2",
    "@supabase/supabase-js": "^2.50.2",
    "@tiptap/extension-image": "^2.23.1",
    "@tiptap/extension-link": "^2.23.1",
    "@tiptap/extension-placeholder": "^2.23.1",
    "@tiptap/react": "^2.23.1",
    "@tiptap/starter-kit": "^2.23.1",
    "axios": "^1.10.0",
    "babel-plugin-styled-components": "^2.1.4",
    "bootstrap": "^5.3.3",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.0",
    "fs": "^0.0.1-security",
    "highlight.js": "^11.9.0",
    "lexical": "^0.32.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "next": "^14.1.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-resizable": "^3.0.5",
    "react-select": "^5.10.1",
    "react-slick": "^0.30.3",
    "react-toggle": "^4.1.3",
    "sass": "^1.72.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.19",
    "swiper": "^11.2.8",
    "swr": "^2.2.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-toggle": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "lib/**/*.{js,jsx,ts,tsx}",
      "pages/**/*.{js,jsx,ts,tsx}",
      "!pages/_app.tsx",
      "!pages/_document.tsx",
      "!**/*.d.ts"
    ]
  }
}
